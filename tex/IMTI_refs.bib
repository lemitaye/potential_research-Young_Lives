	url = {https://doi.org/10.1093/jeea/jvz058},
	doi = {10.1093/jeea/jvz058},
	abstract = {Twin births are often used as an instrument to address selection of women into fertility. However, recent work shows selection of women into twin birth such that, while OLS estimates tend to be downward biased, twin-IV estimates will tend to be upward biased. This is pertinent given the emerging consensus that fertility has limited impacts on women’s labour supply, or on investments in children. Using data for developing countries and the United States to estimate the trade-off between fertility and children’s human capital, we demonstrate the nature and size of the bias in the twin-IV estimator and estimate bounds on the true parameter.},
	number = {6},
	urldate = {2022-03-07},
	journal = {Journal of the European Economic Association},
	author = {Bhalotra, Sonia and Clarke, Damian},
	month = dec,
	year = {2020},
	pages = {3090--3139},
	file = {Full Text PDF:C\:\\Users\\tayel\\Zotero\\storage\\IIVBJPRI\\Bhalotra and Clarke - 2020 - The Twin Instrument Fertility and Human Capital I.pdf:application/pdf;Snapshot:C\:\\Users\\tayel\\Zotero\\storage\\C6W5WU9U\\5686856.html:text/html},
}

@article{clarke_children_2018,
	title = {Children and {Their} {Parents}: {A} {Review} of {Fertility} and {Causality}},
	volume = {32},
	issn = {1467-6419},
	shorttitle = {Children and {Their} {Parents}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/joes.12202},
	doi = {10.1111/joes.12202},
	abstract = {Childbearing decisions are not made in isolation. They are taken in concert with decisions regarding work, marriage, health investments and stocks, as well as many other observable and non-observable considerations. Drawing causal inferences regarding the effect of additional children on family outcomes is complicated by these endogenous factors. This paper lays out the issues involved in estimating the effect of additional child births on family outcomes, and the assumptions underlying the range of estimators and methodologies proposed in the economic literature. The common pitfalls of these estimators are discussed, as well as their potential to bias our interpretation of the effect additional births have on children and parents, both in the existing literature and in future work in the face of changing patterns of childbearing and child-rearing.},
	language = {en},
	number = {2},
	urldate = {2022-03-07},
	journal = {Journal of Economic Surveys},
	author = {Clarke, Damian},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/joes.12202},
	keywords = {Fertility, Causality, Childbirth, Contraceptives},
	pages = {518--540},
	file = {Full Text PDF:C\:\\Users\\tayel\\Zotero\\storage\\L8WBZJEK\\Clarke - 2018 - Children and Their Parents A Review of Fertility .pdf:application/pdf;Snapshot:C\:\\Users\\tayel\\Zotero\\storage\\L2BQB5W3\\joes.html:text/html},
}

@article{bhalotra_twin_2019,
	title = {Twin {Birth} and {Maternal} {Condition}},
	volume = {101},
	issn = {0034-6535},
	url = {https://doi.org/10.1162/rest_a_00789},
	doi = {10.1162/rest_a_00789},
	abstract = {Twin births are often construed as a natural experiment in the social and natural sciences on the premise that the occurrence of twins is quasi-random. We present population-level evidence that challenges this premise. Using individual data for 17 million births in 72 countries, we demonstrate that indicators of mother's health, health-related behaviors, and the prenatal environment are systematically positively associated with twin birth. The associations are sizable, evident in richer and poorer countries—evident even among women who do not use in vitro fertilization—and hold for numerous different measures of health. We discuss potential mechanisms, showing evidence that favors selective miscarriage.},
	number = {5},
	urldate = {2022-03-09},
	journal = {The Review of Economics and Statistics},
	author = {Bhalotra, Sonia and Clarke, Damian},
	month = dec,
	year = {2019},
	pages = {853--864},
	file = {Full Text PDF:C\:\\Users\\tayel\\Zotero\\storage\\AGTF2CN7\\Bhalotra and Clarke - 2019 - Twin Birth and Maternal Condition.pdf:application/pdf;Snapshot:C\:\\Users\\tayel\\Zotero\\storage\\QAWM3BJW\\Twin-Birth-and-Maternal-Condition.html:text/html},
}

