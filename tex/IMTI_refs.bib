	doi = {10.3386/w21663},
	note = {Series: Working Paper Series},
	file = {Full Text PDF:C\:\\Users\\tayel\\Zotero\\storage\\UCNSI8QH\\Bisbee et al. - 2015 - Local Instruments, Global Extrapolation External .pdf:application/pdf},
}

