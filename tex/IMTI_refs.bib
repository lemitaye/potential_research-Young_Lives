@Article{Huber2015,
  author   = {Huber, Martin},
  journal  = {{LABOUR}},
  title    = {Testing the Validity of the Sibling Sex Ratio Instrument},
  year     = {2015},
  issn     = {1467-9914},
  note     = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/labr.12045},
  number   = {1},
  pages    = {1--14},
  volume   = {29},
  abstract = {We test the validity of the sibling sex ratio instrument suggested by Angrist and Evans using the methods proposed by Kitagawa and Huber and Mellace. The sex ratio of the first two siblings is arguably randomly assigned and influences the probability of having a third child, which makes it a candidate instrument for fertility when estimating the effect of fertility on female labor supply. However, identification hinges on the random assignment of the instrument, an instrumental exclusion restriction, and the monotonicity of fertility in the instrument. We find that the instrumental variable tests of Kitagawa and Huber and Mellace do not point to a violation of these assumptions in the Angrist and Evans data (which can, however, not be ruled out even asymptotically as the tests cannot detect all possible violations).},
  doi      = {10.1111/labr.12045},
