@article{caceres-delpiano_impacts_2006,
	title = {The {Impacts} of {Family} {Size} on {Investment} in {Child} {Quality}},
	volume = {41},
	issn = {0022-166X},
	url = {https://www.jstor.org/stable/40057289},
	abstract = {Using multiple births as an exogenous shift in family size, I investigate the impact of the number of children on child investment and child well-being. Using data from the 1980 US Census Five-Percent Public Use Micro Sample, 2SLS results demonstrate that parents facing a change in family size reallocate resources in a way consistent with Becker's Quantity \& Quality model. A larger family generated by twins in a later birth reduces the likelihood that older children attend private school, reduces the mother's labor force participation, and increases the likelihood that parents divorce. The impact of family size on a measure of child outcome, such as grade retention, is less clear. The results indicate that for both measures of child investment and child well-being, the 2SLS estimates are statistically distinguishable from OLS estimates, indicating an omitted variables bias in the single equation model.},
	number = {4},
	urldate = {2022-02-21},
	journal = {The Journal of Human Resources},
	author = {Cáceres-Delpiano, Julio},
	year = {2006},
	note = {Publisher: [University of Wisconsin Press, Board of Regents of the University of Wisconsin System]},
	pages = {738--754},
	file = {JSTOR Full Text PDF:C\:\\Users\\tayel\\Zotero\\storage\\AKYV3HM5\\Cáceres-Delpiano - 2006 - The Impacts of Family Size on Investment in Child .pdf:application/pdf},
}

@article{bhalotra_twin_2020,
	title = {The {Twin} {Instrument}: {Fertility} and {Human} {Capital} {Investment}},
