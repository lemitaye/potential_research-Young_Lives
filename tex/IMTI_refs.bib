@techreport{bisbee_local_2015,
	type = {Working {Paper}},
	title = {Local {Instruments}, {Global} {Extrapolation}: {External} {Validity} of the {Labor} {Supply}-{Fertility} {Local} {Average} {Treatment} {Effect}},
